terraform {
  cloud {
    organization = "apeach"
    workspaces {
      name = "terraform-aws-full-stack"
    }
  }

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4.16"
    }
  }

  required_version = ">= 1.2.0"
}

provider "aws" {
  region = "ap-northeast-2"
}
